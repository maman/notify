{
  "mcpServers": {
    "sosumi": {
      "type": "http",
      "url": "https://sosumi.ai/mcp"
    },
    "XcodeBuildMCP": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "xcodebuildmcp@latest"
      ],
      "env": {
        "INCREMENTAL_BUILDS_ENABLED": "true",
        "XCODEBUILDMCP_SENTRY_DISABLED": "true",
        "XCODEBUILDMCP_DYNAMIC_TOOLS": "true"
      }
    }
  }
}